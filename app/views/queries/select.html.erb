
<section class="content">
<h1>Listing Queries</h1>
  <div class = "row">
    <div class = "col-md-12 col-xs-12">
      <div class="box box-primary">
        <div class="box-header">

          <div class="box-title"> <h3>Table_names</h3> </div>
        </div>
        <div class = "box-body">


				<table class="table">
					<% if @table.size != 0 %>
						<% @table[0].each_key do |key| %>
							<th> <%= key %> </th>
						<% end %>
					<% end %>
					<tbody>
						<% @table.each do |row| %>
							<tr>
							<% row.each_value do |value| %>	
								<td>
									<%= value %>
								</td>
							<% end %>
							</tr>
						<%end%>
					</tbody>
				</table>

				
			</div>
		</div>
	</div>
</div>
